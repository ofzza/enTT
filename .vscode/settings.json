{
  "files.exclude": {
    ".angular": true,
    ".git": true,
    "node_modules": true,
    "dist": true,
    "docs": true
  },

  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,

  "prettier.resolveGlobalModules": false,
  "editor.formatOnSave": true,

  "cSpell.words": [
    "ofzza",
    "region",
    "endregion",
    "instanceof",
    "entt",
    "enttclass",
    "serializable",
    "deserialization",
    "deserializable",
    "deserializes",
    "validate",
    "nonenumerable",
    "writeonly",
    "callsign",
    "Delorean"
  ]
}
