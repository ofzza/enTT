{ "DOCS": "https://github.com/htmllint/htmllint/wiki/Options",
  
  "attr-bans": [],
  "attr-name-ignore-regex": "{{.*?}}",
  "attr-name-style": "dash",
  "attr-new-line": false,
  "attr-no-dup": true,
  "attr-no-unsafe-char": true,
  "attr-order": ["type", "id", "name", "class", "ng-class", "style", "ng-style"],
  "attr-quote-style": "double",
  "attr-req-value": true,
  "attr-validate": true,
  "class-no-dup": true,
  "class-style": "dash",
  "doctype-first": "smart",
  "doctype-html5": true,
  "fig-req-figcaption": true,
  "focusable-tabindex-style": true,
  "head-req-title": true,
  "head-valid-content-model": true,
  "href-style": false,
  "html-req-lang": false,
  "html-valid-content-model": true,
  "id-class-ignore-regex": false,
  "id-class-no-ad": true,
  "id-class-style": "dash",
  "id-no-dup": true,
  "img-req-alt": true,
  "img-req-src": true,
  "indent-style": "spaces",
  "indent-width": 2,
  "indent-width-cont": true,
  "input-radio-req-name": true,
  "input-req-label": false,
  "label-req-for": false,
  "lang-style": "case",
  "line-end-style": "lf",
  "line-max-len": 80,
  "line-max-len-ignore-regex": false,
  "maxerr": 32,
  "raw-ignore-regex": "\\<\\!-- htmllint ignore --\\>[^]*?\\<\\!-- htmllint unignore --\\>",
  "spec-char-escape": true,
  "table-req-caption": false,
  "table-req-header": false,
  "tag-bans": ["style", "b", "iframe"],
  "tag-close": true,
  "tag-name-lowercase": true,
  "tag-name-match": true,
  "tag-self-close": true,
  "text-ignore-regex": "\\<\\!-- htmllint ignore --\\>[^]*?\\<\\!-- htmllint unignore --\\>",
  "title-max-len": 60,
  "title-no-dup": true
}
