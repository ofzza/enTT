  },
  "homepage": "https://github.com/ofzza/enTT#readme",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.11.8",
    "eslint": "^8.26.0",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "joi": "^17.6.4",
    "joi-browser": "^13.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
